= form_for :search_accounts, url: account_search_path, method: :get do |form|
  .input-group.mb-4
    .d-flex
      = form.text_field :query, value: params[:search_accounts].presence && params[:search_accounts][:query], placeholder: "Search", class: 'form-control me-3'
      = form.submit 'Search', class: 'btn btn-primary'
